CREATE TABLE data (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name VARCHAR (100) NOT NULL,
    height INTEGER,
    weight FLOAT,
    birthdate DATE,
    married BOOlEAN DEFAULT FALSE);




